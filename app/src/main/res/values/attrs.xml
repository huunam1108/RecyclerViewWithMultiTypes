<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewPagerIndicator">
        <attr format="color" name="selectedColor"/>
        <attr format="color" name="deselectedColor"/>
        <attr format="reference" name="selectedDrawable"/>
        <attr format="reference" name="deselectedDrawable"/>
        <attr format="dimension" name="indicatorSpacing"/>
        <attr format="boolean" name="enableAnimation"/>
        <attr format="integer" name="animationDuration"/>
        <attr format="float" name="animationScale"/>
        <attr format="color" name="backgroundColor"/>
    </declare-styleable>
    <declare-styleable name="InfiniteViewPager">
        <attr format="boolean" name="autoScroll"/>
        <attr format="boolean" name="autoSmoothScroll"/>
        <attr format="integer" name="autoScrollInterval"/>
    </declare-styleable>
    <declare-styleable name="NetWorkStateView">
        <attr format="reference" name="layoutLoading"/>
        <attr format="reference" name="layoutRetry"/>
        <attr format="reference" name="layoutInfo"/>
    </declare-styleable>
</resources>